<Window x:Class="VPN_Setup.DestinationLocationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VPN_Setup"
        mc:Ignorable="d"
        Title="DestinationLocationWindow" Height="450" Width="800" ResizeMode="CanMinimize" Closing="Window_Closing">
    <Grid>
        <TextBox x:Name="LocationTB" HorizontalAlignment="Left" Height="34" Margin="89,187,0,0" TextWrapping="Wrap" Text="C:\Program Files(x86)\VPN Unlimited" VerticalAlignment="Top" Width="382"/>
        <Button x:Name="BrowseButton" Content="Browse..." HorizontalAlignment="Left" Margin="529,202,0,0" VerticalAlignment="Top" Width="75" Click="BrowseButton_Click"/>
        <Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="709,391,0,0" VerticalAlignment="Top" Width="75" Click="CancelButton_Click"/>
        <Button x:Name="NextButton" Content="Next" HorizontalAlignment="Left" Margin="611,391,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.152,0.583" Click="NextButton_Click"/>
        <Button x:Name="BackButton" Content="Back" HorizontalAlignment="Left" Margin="531,391,0,0" VerticalAlignment="Top" Width="75" Click="BackButton_Click"/>

    </Grid>
</Window>
