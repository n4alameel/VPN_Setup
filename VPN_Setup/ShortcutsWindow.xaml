<Window x:Class="VPN_Setup.ShortcutsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VPN_Setup"
        mc:Ignorable="d"
        Title="ShortcutsWindow" Height="450" Width="800" Closing="Window_Closing">
    <Grid>
        <TextBox x:Name="ShortcutTB" HorizontalAlignment="Left" Height="32" Margin="82,226,0,0" TextWrapping="Wrap" Text="VPN Unlimited" VerticalAlignment="Top" Width="435"/>
        <Button x:Name="BrowseButton" Content="Browse..." HorizontalAlignment="Left" Margin="594,239,0,0" VerticalAlignment="Top" Width="75" Click="BrowseButton_Click"/>
        <Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="709,391,0,0" VerticalAlignment="Top" Width="75" Click="CancelButton_Click"/>
        <Button x:Name="NextButton" Content="Next" HorizontalAlignment="Left" Margin="606,391,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="BackButton" Content="Back" HorizontalAlignment="Left" Margin="526,391,0,0" VerticalAlignment="Top" Width="75" Click="BackButton_Click"/>

    </Grid>
</Window>
